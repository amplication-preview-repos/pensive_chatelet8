datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model SizeRecommendation {
  clothingItem    String?
  createdAt       DateTime @default(now())
  id              String   @id @default(cuid())
  recommendedSize String?
  updatedAt       DateTime @updatedAt
  user            String?
}
